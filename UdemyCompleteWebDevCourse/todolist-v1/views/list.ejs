<%- include("header") -%>

    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
      <h2><%=currentDate%></h2>
    </div>

    <div class="box">
      <% for(i=0; i<newListItems.length; i++){ %>
      <div class="item">
          <input type="checkbox">
          <p><%= newListItems[i] %></p>
      </div>
      <% } %>



      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="Add Task Here" autocomplete="off">
        <button type="submit" name="name" value=<%= listTitle %>>+</button>
      </form>
    </div>

<%- include("footer") -%>
