<%- include("partials/header") %>

<h1>Compose</h1>



  <form class="composing" action="/compose" method="post">
    <div class="form-group">
      <label for="blogPostTitle">Title</label>
      <input class="form-control" id="blogPostTitle" type="text" name="blogPostTitle" autocomplete="off">
      <label for="blogPostContent">Post</label>
      <textarea class="form-control" name="blogPostContent" rows="5" cols="40" autocomplete="off"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </form>



<%- include("partials/footer") %>
